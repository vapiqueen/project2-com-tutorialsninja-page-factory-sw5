<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1" >
    <listeners>
        <listener class-name="com.tutorialsninja.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>
    <test name = "Tutorialninja Test" >
        <classes>
            <class name="com.tutorialsninja.testsuite.AccountLoginTest"/>
            <class name="com.tutorialsninja.testsuite.AccountRegisterTest"/>
            <class name="com.tutorialsninja.testsuite.DesktopTest"/>
            <class name="com.tutorialsninja.testsuite.LaptopsAndNotebooksTest"/>
            <class name="com.tutorialsninja.testsuite.TopMenuTest"/>

        </classes>
    </test>
</suite>